  (let [l #py [1 2 3]]
    (is (= :a (aset l 0 :a)))
    (is (= #py [:a 2 3] l))
    (is (thrown? python/IndexError (aset l 5 :f))))

  (let [l #py [#py [:a :b :c] #py [:d :e :f]]]
    (is (= 5 (aset l 0 2 5)))
    (is (= #py [#py [:a :b 5] #py [:d :e :f]] l))
    (is (thrown? python/IndexError (aset l 0 5 :cc)))))

