(deftest eduction-test
  (is (= '(1 9 49 81)
         (seq (eduction (mapcat identity)
                        (filter odd?)
                        (map (fn [x] (* x x)))
                        (take 5)
                        [(range 5) (range 6 10)]))))
  (is (= '(1 9 25 49 81)
         (seq (eduction (map inc)
                        (filter odd?)
                        (map (fn [x] (* x x)))
                        (take 5)
                        (range)))))
  (is (= '(1 9 49 81 121)
         (seq (eduction (mapcat identity)
                        (filter odd?)
                        (map (fn [x] (* x x)))
                        (take 5)
                        [(range 5) (range 6 10) (range 11 20)]))))
  (is (= '(1 9 49 81 121 169 225 289 361)
         (seq (eduction (mapcat identity)
                        (filter odd?)
                        (map (fn [x] (* x x)))
                        [(range 5) (range 6 10) (range 11 20)])))))

(deftest halt-when-test
  (is (= 5 (transduce (comp (map inc)
                            (halt-when #(>= % 5)))
                      +
                      0
                      (range))))
  (is (= 10 (transduce (comp (map inc)
                             (halt-when #(>= % 5)
                                        (fn [result _] result)))
                       +
                       0
                       (range)))))

