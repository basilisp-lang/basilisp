      (is (= [11] (vec (pmap slow [1]))))
      (is (= [11 12 13 14 15 16 17 18 19]
             (vec (pmap slow (range 1 10))))))))

(deftest pcalls-test
  (binding [*pmap-cpu-count* 2]
    (let [slow (fn slow [x]
                 (time/sleep 0.5)
